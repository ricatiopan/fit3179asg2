{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Suicide Rates 2016-2020 in Australia per 100.000 people",
    "width": 500,
    "height": 350,
    "data": {
      "url": "https://raw.githubusercontent.com/ricatiopan/fit3179asg2/main/js/SA3_2016_AUST.json",
      "format": {"type": "topojson", "feature": "SA3_2016_AUST"}
    },
   
    "transform": [
      {
        "lookup": "properties.SA3_NAME16",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/ricatiopan/fit3179asg2/main/data/SA3.csv"
          },
          "key": "SA3 Name",
          "fields": ["SA3 ASR"]
        }
      }
    ],
    "projection": {"type": "equirectangular"},
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "SA3 ASR",
        "type": "quantitative",
        "scale": {"type": "linear"}
      },
      "tooltip": [
        {"field": "properties.SA3_NAME16", "type": "nominal", "title": "SA3"},
        {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
        {"field": "SA3 ASR", "type": "quantitative", "title": "Suicide Rate"}
      ]
    }
  }