{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "data": {"url": "https://raw.githubusercontent.com/ricatiopan/fit3179asg2/main/data/agegroup.csv"},
    "title":"Suicide Rate Age Distribution",
    "mark": "rect",
    "encoding": {
      "y": {"field": "age_group", "type": "nominal", "title":"Age Group"},
      "x": {"field": "Year", "type": "ordinal"},
      "color": {"field": "Rate", "type": "quantitative"},
      "tooltip": [
        {"field": "Year", "type": "ordinal", "title": "Year"},
        {"field": "age_group", "type": "nominal", "title": "Age Group"},
        {"field": "Rate", "type": "quantitative", "format": ".2f"}
      ]
    }
  }